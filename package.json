    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 5 version",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
